<schemalist>
  <schema gettext-domain="@GETTEXT_PACKAGE@" id="org.cafe.SettingsDaemon.plugins.keybindings" path="/org/cafe/settings-daemon/plugins/keybindings/">
    <key name="active" type="b">
      <default>true</default>
      <summary>Activation of this plugin</summary>
      <description>Whether this plugin would be activated by cafe-settings-daemon or not</description>
    </key>
    <key name="priority" type="i">
      <default>6</default>
      <summary>Priority to use for this plugin</summary>
      <description>Priority to use for this plugin in cafe-settings-daemon startup queue</description>
    </key>
  </schema>
</schemalist>
